# Backend selection: transformers (default) or vllm
BACKEND=transformers

# Hugging Face model configuration
MODEL_ID=deepseek-ai/DeepSeek-OCR
TORCH_DTYPE=bf16
DEVICE_MAP=auto

# Operational limits
MAX_CONCURRENCY=2
REQUEST_TIMEOUT=60

# PDF handling
ENABLE_PDF_SUPPORT=1
PDF_MAX_PAGES=20
PDF_DPI_DEFAULT=180

# Observability
METRICS_ENABLED=1
CORS_ALLOW_ORIGINS=
